        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.2",
        "ts-node": "^8.10.2",
        "typeorm": "^0.2.25",
        "typescript": "^3.9.7",
        "uuid": "^8.2.0",
        "yarn": "^1.22.4"
    },
    "devDependencies": {
        "@nestjs/schematics": "^7.0.1",
